# Application Configuration
VITE_APP_TITLE=GojoStreamNet
VITE_APP_DESCRIPTION=Modern anime streaming platform

# API Endpoints (no API keys required for public APIs)
VITE_HIANIME_API_URL=https://hianime-api-jzl7.onrender.com/api/v1
VITE_YUMA_API_URL=https://yumaapi.vercel.app

# Services
VITE_IMAGE_OPTIMIZATION_SERVICE=https://images.weserv.nl

# Optional: Custom CORS proxy URL for M3U8 streams
VITE_APP_CORS_PROXY_URL=/api/proxy

# Development
VITE_ENABLE_PERFORMANCE_MONITOR=true
VITE_ENABLE_DEBUG_LOGS=false

# Analytics (Optional)
# VITE_GOOGLE_ANALYTICS_ID=
# VITE_SENTRY_DSN=

# Feature Flags
VITE_ENABLE_EXPERIMENTAL_FEATURES=false