{
  "name": "gojostreamnet",
  "version": "1.0.0",
  "description": "Modern anime streaming platform with comprehensive error handling and performance optimization",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/patheticnoob/gojoStreamNet",
  "repository": {
    "type": "git",
    "url": "https://github.com/patheticnoob/gojoStreamNet.git"
  },
  "keywords": [
    "anime",
    "streaming",
    "react",
    "typescript",
    "material-ui",
    "vite",
    "performance"
  ],
  "author": "patheticnoob",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@types/lodash": "^4.17.20",
    "framer-motion": "^7.1.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.9.0",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "video.js": "^8.3.0",
    "videojs-youtube": "^3.0.1",
    "vite-tsconfig-paths": "^3.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-slick": "^0.23.10",
    "@vitejs/plugin-react": "^2.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.1"
  }
}
